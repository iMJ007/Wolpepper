            android:padding="8dp">

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/eclectik"
                android:fontFamily="@font/spacemono_bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp"
                android:alpha="0.2"
                android:text="@string/developers_eclectik"
                android:textColor="#fafafa"
                android:textSize="14sp"
                 />

            <LinearLayout
                android:id="@+id/active_dev_category"
                android:layout_width="match_parent"
                android:layout_below="@id/eclectik"
                android:layout_marginTop="12dp"
                android:layout_height="wrap_content"
                android:gravity="center">
                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="#fafafa"
                    android:alpha="0.1" />

                <com.google.android.material.textview.MaterialTextView
                    android:fontFamily="@font/spacemono_bold"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Active developer and support"
                    android:alpha="0.4"
                    android:textSize="12sp"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="8dp"
                    android:textColor="#fafafa"/>

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="#fafafa"
                    android:alpha="0.1" />

            </LinearLayout>

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/author_image_1"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginEnd="8dp"
                android:layout_below="@id/active_dev_category"
                android:src="@drawable/author_2"
                android:layout_marginTop="12dp"
