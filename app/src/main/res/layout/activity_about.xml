                android:layout_marginTop="8dp"
                android:alpha="0.7"
                android:background="?selectableItemBackgroundBorderless"
                android:gravity="center"
                android:text="@string/website"
                android:textColor="@color/colorAccent"
                 />

            <LinearLayout
